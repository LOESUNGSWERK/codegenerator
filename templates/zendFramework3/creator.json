{
  "creator": "LÖSUNGSWERK",
  "user": "Rene Kühle",
  "caption": "zendFrameWork3-MVC",
  "description": "baut ein MVC-Pattern für zendFramework3",
  "target": "./zendFramework/",
  "defaults": {
    "table": {
      "isDepricated": false,
      "tableType": "table",
      "extraInformation": {
        "hasPictureliste": true,
        "isDistributable": true
      }
    },
    "field": {
        "defaultValue": null,
        "isAutoinc": false,
        "isPrimaryKey": false,
        "isIndex": false,
        "canBeNull": false
    },
    "reference": {
        "onDelete": null,
        "onUpdate": null,
        "onInsert": null
    }
  },
  "tasks": [
    {
      "caption": "Entity",
      "insertTemplateFile": "Entity/Entity.html",
      "destinationFile": "module/{{modul|ucf}}/src/Model/{{tableName|ucf}}/Entity/{{tableName|ucf}}Entity.php",
      "onUpdate": "OVERWRIDE",
      "aktiv": true
    },
    {
      "caption": "StorageFactory",
      "insertTemplateFile": "Storage/StorageFactory.html",
      "destinationFile": "module/{{modul|ucf}}/src/Model/{{tableName|ucf}}/Storage/{{tableName|ucf}}StorageFactory.php",
      "onUpdate": "OVERWRIDE",
      "aktiv": true
    },
    {
      "caption": "Storage",
      "insertTemplateFile": "Storage/Storage.html",
      "destinationFile": "module/{{modul|ucf}}/src/Model/{{tableName|ucf}}/Storage/{{tableName|ucf}}Storage.php",
      "onUpdate": "OVERWRIDE",
      "aktiv": true
    },
    {
      "caption": "Repository",
      "insertTemplateFile": "Repository/Repository.html",
      "destinationFile": "module/{{modul|ucf}}/src/Model/{{tableName|ucf}}/Repository/{{tableName|ucf}}Repository.php",
      "onUpdate": "OVERWRIDE",
      "aktiv": true
    },
    {
      "caption": "RepositoryFactory",
      "insertTemplateFile": "Repository/RepositoryFactory.html",
      "destinationFile": "module/{{modul|ucf}}/src/Model/{{tableName|ucf}}/Repository/{{tableName|ucf}}RepositoryFactory.php",
      "onUpdate": "OVERWRIDE",
      "aktiv": true
    }
  ]
}